{
	"name": "kartotherian",
	"publishConfig": {
		"access": "public"
	},
	"version": "2.0.0",
	"main": "./app.js",
	"scripts": {
		"start": "service-runner",
		"lint": "eslint --max-warnings 0 . && stylelint **/*.css",
		"lint:fix": "eslint --fix . && stylelint --fix **/*.css",
		"test": "npm run lint && jest --maxWorkers=1",
		"docker-start": "service-runner docker-start",
		"docker-test": "service-runner docker-test",
		"test-build": "service-runner docker-test && service-runner build --deploy-repo --force",
		"coverage": "jest --coverage --maxWorkers=1"
	},
	"description": "Kartotherian - a suite of map tile rendering and map serving software",
	"license": "Apache-2.0",
	"repository": "wikimedia/mediawiki-services-kartotherian",
	"bugs": "https://phabricator.wikimedia.org/tag/maps/",
	"homepage": "https://github.com/kartotherian/kartotherian",
	"keywords": [
		"maps",
		"tilelive",
		"Wikipedia",
		"MediaWiki",
		"kartotherian"
	],
	"author": "Yuri Astrakhan <YuriAstrakhan@gmail.com>",
	"contributors": [
		"Max Semenik <msemenik@wikimedia.org>",
		"Michael Holloway <mholloway@wikimedia.org>",
		"Mateus Santos <msantos@wikimedia.org>",
		"Petr Pchelko <ppchelko@wikimedia.org>",
		"Stephane Bisson <sbisson@wikimedia.org>"
	],
	"dependencies": {
		"@kartotherian/abaculus": "^3.0.3-alpha.0",
		"@kartotherian/mapnik": "~3.7.4",
		"@kartotherian/osm-bright-source": "^1.0.8-wmf.0",
		"@kartotherian/osm-bright-style": "^4.0.2",
		"@kartotherian/tilelive-bridge": "~3.1.0",
		"@kartotherian/tilelive-tmsource": "^1.0.2-alpha.0",
		"@kartotherian/tilelive-vector": "^4.0.3-alpha.0",
		"@mapbox/geo-viewport": "^0.5.0",
		"@mapbox/node-pre-gyp": "^1.0.10",
		"@mapbox/tilejson": "^1.0.3",
		"@mapbox/tilelive": "^6.1.1",
		"@turf/bbox": "^6.5.0",
		"@wikimedia/geojson-mapnikify": "^3.0.1",
		"@wikimedia/makizushi": "4.0.0",
		"@wikimedia/mapdata": "^0.8.0",
		"@wikimedia/tilelive-overlay": "^2.3.0",
		"bluebird": "^3.5.1",
		"body-parser": "^1.20.1",
		"browserify": "^17.0.0",
		"bunyan": "^1.8.9",
		"cassandra-uuid": "^0.0.2",
		"compression": "^1.6.2",
		"domain-validator": "^0.0.5",
		"domino": "^1.0.28",
		"express": "^4.18.2",
		"js-yaml": "^3.12.2",
		"language-scripts": "^1.0.2",
		"leaflet": "^1.9.2",
		"libxmljs": "0.19.5",
		"mwapi": "^0.0.10",
		"pbf": "^3.2.0",
		"pg-promise": "^9.3.6",
		"pg.js": "^4.1.1",
		"preq": "^0.5.2",
		"quadtile-index": "^0.0.6",
		"service-runner": "^2.9.0",
		"tilelive-http": "~0.13.0",
		"tilelive-promise": "^2.0.0",
		"tilelive-tmstyle": "0.8.0",
		"to-regex-range": "^4.0.2",
		"topojson-server": "^2.0.0",
		"typedarray-to-buffer": "^3.1.2",
		"underscore": "^1.13.6",
		"wd-type-parser": "^0.0.3"
	},
	"optionalDependencies": {
		"bunyan-prettystream": "*",
		"@mapbox/sphericalmercator": "^1.2.0"
	},
	"devDependencies": {
		"chai": "^4.3.6",
		"eslint": "^7.32.0",
		"eslint-config-wikimedia": "^0.20.0",
		"extend": "^3.0.0",
		"jest": "^27.3.1",
		"json-stringify-pretty-compact": "^4.0.0",
		"openapi-schema-validator": "^12.0.2",
		"request": "^2.88.2",
		"request-promise": "^4.2.2",
		"stylelint": "^13.13.1",
		"stylelint-config-wikimedia": "^0.11.1",
		"swagger-router": "^0.7.4"
	},
	"deploy": {
		"node": "10.15.2",
		"target": "debian:buster",
		"install_opts": [
			"--build-from-source=@kartotherian/mapnik"
		],
		"dependencies": {
			"_all": [
				"fonts-dejavu",
				"libboost-filesystem-dev",
				"libboost-regex-dev",
				"libboost-system-dev",
				"libcairo2-dev",
				"libfreetype6-dev",
				"libgdal-dev",
				"libharfbuzz-dev",
				"libjpeg-dev",
				"libpng-dev",
				"libpq-dev",
				"libproj-dev",
				"libtiff-dev",
				"libwebp-dev",
				"libxml2-dev",
				"libmapbox-variant-dev",
				"libboost-program-options-dev",
				"libboost-thread-dev",
				"libmapnik-dev",
				"mapnik-utils",
				"mapnik-doc",
				"libmapnik3.0"
			]
		}
	}
}
